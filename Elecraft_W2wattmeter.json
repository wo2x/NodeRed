    {
        "id": "241d19be.fee786",
        "type": "function",
        "z": "4edba573.cf19bc",
        "name": "Parse Decimal FWD",
        "func": "msg.payload = (msg.payload.toString('utf8'));\nmsg.divide = (msg.payload.substr(7,1));\nmsg.divide = parseFloat(msg.divide,10);\n//msg.divide = (msg.divide -1);\nvar divisor = msg.divisor ;\ndivisor = Math.pow(10, (msg.divide));\nflow.set(\"divisor\",divisor);\nmsg.divisor = divisor;\n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "x": 540,
        "y": 700,
        "wires": [
            [
                "dd651118.73661"
            ]
        ]
    },
    {
        "id": "154c698a.255766",
        "type": "function",
        "z": "4edba573.cf19bc",
        "name": "Change Scaling",
        "func": "msg.payload = msg.payload.toString('utf8');\n\nmsg.range = (msg.payload.substr(2,1));\nmsg.range = parseInt(msg.range,10);\nmsg.type = (msg.payload.substr(4,1));\nmsg.type = parseInt(msg.type,10);\n\n{if (msg.range == 1) {\n    msg.control = {min:0, max:2, seg1:1, seg2:1.5}\n}\nelse if (msg.range == 2) {\n    msg.control = {min:0, max:20, seg1:10, seg2:15}\n}\n    else if (msg.range == 3) {\n        msg.control = {min:0, max:200, seg1:100, seg2:150}\n}\n    else if (msg.range == 4) {\n        msg.control = {min:0, max:2000, seg1:1000, seg2:1500}\n    }\n    }\n    \n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "x": 520,
        "y": 820,
        "wires": [
