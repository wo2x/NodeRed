        "id": "dd651118.73661",
        "type": "function",
        "z": "4edba573.cf19bc",
        "name": "Parse FWD Power",
        "func": "msg.payload = msg.payload.toString('utf8');\n\nmsg.payload = (msg.payload.substr(1,5));\nmsg.payload = parseInt(msg.payload,10);\n\nvar divisor = flow.get(\"divisor\") ;\nmsg.payload = msg.payload  / divisor;\nmsg.forward = msg.payload\n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "x": 829.9999694824219,
